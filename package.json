{
    "name": "one-guide",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "test": "jest",
        "rm-i": "rm -rf node_modules/ && npm install",
        "android": "react-native run-android --variant=devDebug",
        "android-staging": "Set ENVFILE=.env.staging && react-native run-android --variant=stagingDebug",
        "android-prod": "ENVFILE=.env.production react-native run-android --variant=productionDebug && adb shell am start -n com.oneguide/com.oneguide.dev.MainActivity && cd ..",
        "android-build": "export ENVFILE=.env && cd android && ./gradlew assembleDevRelease && cd ..",
        "android-build-staging": "export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
        "android-build-prod": "export ENVFILE=.env.production && cd android && ./gradlew assembleProductionRelease && cd ..",
        "ios": "react-native run-ios --configuration Debug --scheme OneGuide",
        "ios-6": "react-native run-ios --configuration Debug --scheme OneGuide --simulator='iPhone 6'",
        "ios-staging": "react-native run-ios --configuration Staging --scheme 'OneGuide (Staging)'",
        "ios-prod": "react-native run-ios --configuration Production --scheme 'OneGuide (Production)'"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "i18next": "^15.0.9",
        "moment": "^2.24.0",
        "react": "16.8.6",
        "react-native": "0.59.3",
        "react-native-camera": "^2.2.1",
        "react-native-config": "^0.11.7",
        "react-native-datepicker": "^1.7.2",
        "react-native-dialog-input": "^1.0.7",
        "react-native-fast-image": "^5.1.2",
        "react-native-fetch-blob": "^0.10.8",
        "react-native-gesture-handler": "1.0.16",
        "react-native-get-real-path": "git+https://github.com/Wraptime/react-native-get-real-path.git",
        "react-native-image-gallery": "^2.1.5",
        "react-native-image-resizer": "^1.0.1",
        "react-native-image-zoom-viewer": "^3.0.1",
        "react-native-material-dropdown": "^0.11.1",
        "react-native-navigation": "^2.21.0",
        "react-native-reanimated": "^1.0.0",
        "react-native-render-html": "^4.2.3",
        "react-native-splash-screen": "^3.2.0",
        "react-native-tab-view": "^2.0.3",
        "react-native-vector-icons": "^7.0.0",
        "react-native-webview": "^10.9.0",
        "react-navigation": "^4.4.0",
        "react-navigation-fluid-transitions": "^0.3.2",
        "react-redux": "^6.0.0",
        "react-simple-timefield": "^1.3.2",
        "redux": "^4.0.1",
        "redux-persist": "^5.10.0",
        "redux-thunk": "^2.3.0",
        "socket.io-client": "^2.2.0"
    },
    "devDependencies": {
        "babel-jest": "23.6.0",
        "graceful-fs": "^4.2.4",
        "jest": "23.6.0",
        "metro-react-native-babel-preset": "0.51.1",
        "react-native-gesture-handler": "1.0.16",
        "react-test-renderer": "16.6.3"
    },
    "resolutions": {
        "@babel/helpers": "7.2.0",
        "@babel/runtime": "7.2.0",
        "graceful-fs": "4.2.4"
    },
    "jest": {
        "preset": "react-native"
    }
}
